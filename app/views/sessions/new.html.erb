<div class="container">
  <div class="jumbotron">
    <h1>BTC Manager</h1>
    <p>Bootstrap is the most popular HTML, CSS, and JS framework for developing
    responsive, mobile-first projects on the web.</p>
  </div>
</div>





<h1>Sign up</h1>


<%= render 'form' %>



<%= form_for :login do |f| %>
<h1>Log In</h1>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'Log In', :class=>"btn btn-default" %>
  </div>
<% end %>
